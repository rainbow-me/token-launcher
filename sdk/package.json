{
  "name": "@rainbow-me/token-launcher",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "jsbi": "^4.3.0",
    "tiny-invariant": "^1.3.3",
    "eslint-config-rainbow": "4.3.0",
    "prettier": "^3.3.3",
    "viem": "^2"
  },
  "author": "Christopher Howard",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "build:clean": "yarn clean && yarn build",
    "prepublishOnly": "yarn build:clean",
    "prepare": "yarn build",
    "pretest": "grep -v '^IS_TESTING=' .env > .env.tmp 2>/dev/null || true; echo 'IS_TESTING=true' >> .env.tmp; mv .env.tmp .env",
    "test": "./scripts/test.sh",
    "lint": "eslint 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "types": "tsc --noEmit",
    "format:check": "prettier --check '**/*.{js,ts,json,md}'",
    "format:fix": "prettier --write '**/*.{js,ts,json,md}'",
    "clean": "rm -rf dist"
  },
  "jest": {
    "rootDir": ".",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/helpers/setupTestEnv.ts"
    ]
  },
  "sideEffects": false
}
